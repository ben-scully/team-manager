<p id="notice"><%= notice %></p>

<h1>Squad Members</h1>

<p>
  <%= link_to 'New Squad Member', new_squad_squad_member_path %>
</p>

<h4>List</h4>

<table>
  <tbody>
    <% @squad_members.each do |squad_member| %>
      <tr>
        <td><%= squad_member.display_name %></td>
        <td><%= link_to 'Show', [squad_member.squad, squad_member] %></td>
        <td><%= link_to 'Edit', [:edit, squad_member.squad, squad_member] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
