<p id="notice"><%= notice %></p>

<h1>Squad</h1>

<p>
  <strong>DisplayName:</strong>
  <%= @squad.display_name %>
</p>

<h2>Users</h2>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @squad.users.each do |user| %>
      <tr>
        <td><%= user.display_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Edit', edit_squad_path(@squad) %> |
<%= link_to 'Back', squads_path %>
