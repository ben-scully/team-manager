<p id="notice"><%= notice %></p>

<h1>Game Members</h1>

<p>
  <%= link_to 'New Game member', new_game_game_member_path(@game) %>
</p>

<h4>List</h4>

<table>
  <tbody>
    <% @game.game_members.each do |game_member| %>
      <tr>
        <td><%= game_member.display_name %></td>
        <td><%= link_to 'Show', [game_member.game, game_member] %></td>
        <td><%= link_to 'Edit', [:edit, game_member.game, game_member] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
